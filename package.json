    "eslint": "^8.45.0",
    "jest": "^29.5.0",
    "rimraf": "^5.0.0",
    "rollup": "^3.28.0",
    "rollup-plugin-typescript2": "^0.35.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.0"
  },
  "repository": {
    "type": "git",
